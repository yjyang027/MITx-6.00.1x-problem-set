def calculateHandlen(hand):
    """ 
    Returns the length (number of letters) in the current hand.
    
    hand: dictionary (string int)
    returns: integer
    """
    # TO DO... <-- Remove this comment when you code this function
    hand2 = hand.copy()
    ans = 0
    for key in hand2:
        if hand[key] != 0:
            ans += hand[key]
    return ans
